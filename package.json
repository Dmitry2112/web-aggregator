    "@angular/router": "^17.2.3",
    "@taiga-ui/cdk": "^3.61.0",
    "@taiga-ui/core": "^3.61.0",
    "@taiga-ui/icons": "^3.61.0",
    "@taiga-ui/kit": "^3.61.0",
    "@taiga-ui/styles": "^3.61.0",
    "@tinkoff/ng-dompurify": "3.0.0",
    "dompurify": "2.2.9",
    "rxjs": "~7.8.0",
