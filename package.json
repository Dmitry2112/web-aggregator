{
  "name": "web-aggregator",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "server": "json-server --watch server/db.json --routes server/routes.json -p 3000",
